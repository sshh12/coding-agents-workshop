# Environment configuration for ML Experiment Tracker
# Copy this to .env and update values for your environment

# Database (default: sqlite:///./experiments.db)
DATABASE_URL=sqlite:///./experiments.db

# Security (CHANGE THESE IN PRODUCTION)
SECRET_KEY=change-me-in-production
API_KEY=your-api-key-here

# Feature flags
ENABLE_TAGS=false
ENABLE_EXPORT=false
